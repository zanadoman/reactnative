root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
spelling_language = en-US
trim_trailing_whitespace = true

[*.tsx]
quote_type = double
ij_typescript_use_double_quotes = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false
